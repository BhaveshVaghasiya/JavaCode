package test;

import java.util.HashMap;

class ThredFinalVariable {
	final int num = 40;
	void display() {
		 int num = 10;
		{
			Runnable r = new Runnable() {
				final int num = 20;

				@Override
				public void run() {
					int num = 30;
					System.out.println(num);
				}

			};
			r.run();
		}
	}

	public static void main(String args[]) {
		ThredFinalVariable nj = new ThredFinalVariable();
		nj.display();
	}

}